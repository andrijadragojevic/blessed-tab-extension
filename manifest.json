{
  "manifest_version": 3,
  "name": "Blessed Tab",
  "version": "1.0",
  "description": "A new tab extension displaying a Bible quote with a beautiful background.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}